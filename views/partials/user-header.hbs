        <li class="lvl1 parent megamenu">
            <a href="#">Options <i class="an an-plus"></i></a>
            <ul>
                <li>
                    <a href="#" class="site-nav">My Account <i class="an an-plus"></i></a>
                    <ul>

                        <li><a href="/login" class="site-nav">Login</a></li>
                        <li><a href="/profile" class="site-nav">Profile</a></li>
                        <li><a href="/signup" class="site-nav">Register</a></li>
